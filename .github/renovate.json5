{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    "schedule:daily",
    "schedule:automergeDaily",
    ":enablePreCommit",
    ":maintainLockFilesWeekly",
    ":prHourlyLimitNone",
    ":semanticCommits",
    ":automergeMinor"
  ],
  "labels": [
    "dependencies",
    "renovate"
  ],
  "prConcurrentLimit": 0,
  "assignees": [
    "fadedDexofan"
  ],
  "reviewers": [
    "fadedDexofan"
  ],
  "timezone": "UTC",
  "rangeStrategy": "auto",
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true,
    "automergeType": "pr",
    "schedule": [
      "before 5am on monday"
    ]
  },
  "vulnerabilityAlerts": {
    "labels": [
      "security"
    ],
    "assignees": [
      "fadedDexofan"
    ]
  }
}
